{
  "version": "0.2.0",
  "inputs": [
    {
      "id": "project",
      "type": "pickString",
      "description": "Project",
      "options": [
        "${workspaceFolder}/Vigor.Core.Program.Api.Http"
      ]
    }
  ],
  "configurations": [
    {
      "name": "Launch",
      "type": "coreclr",
      "request": "launch",
      "program": "${input:project}/bin/Debug/net8.0/Vigor.Core.Program.Api.Http.dll",
      "args": [],
      "env": {
        "DOTNET_ENVIRONMENT": "Development"
      },
      "cwd": "${input:project}",
      "stopAtEntry": false,
      "console": "internalConsole"
    }
  ]
}